{
  "name": "jekyll-aws-comments",
  "version": "1.1.0",
  "description": "Static comments for Jekyll with AWS Lambda and GitHub",
  "main": "comments.js",
  "scripts": {
    "clean": "rm -fr node_modules && rm -f jekyll-aws-comments.zip",
    "build-aws-lambda": "repack-zip",
    "test": "mocha",
    "test-config": "node test_config.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mww/jekyll-aws-comments.git"
  },
  "keywords": [
    "jekyll",
    "aws",
    "github",
    "comments"
  ],
  "author": "Michael Ummels <michael@ummels.de>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mww/jekyll-aws-comments/issues"
  },
  "homepage": "http://ummels.github.io/jekyll-aws-comments",
  "private": true,
  "dependencies": {
    "aws-sdk": "^2.645.0",
    "js-md5": "^0.7.3",
    "moment": "^2.24.0",
    "octokat": "^0.10.0"
  },
  "devDependencies": {
    "aws-sdk-mock": "^5.1.0",
    "mocha": "^7.1.1",
    "nock": "^12.0.3",
    "repack-zip": "^0.2.8"
  },
  "engines": {
    "node": ">= 12.1"
  },
  "repackZipConfig": {
    "excludes": [
      "README.md",
      "example_config.json",
      "test/**",
      "node_modules/aws-sdk/**"
    ]
  }
}
